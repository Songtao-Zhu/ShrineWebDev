<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shrine.web.mapper.CastMapper">
    <select id="queryCastsBySeriesId" parameterType="Integer" resultType="com.shrine.web.entity.Cast">
        SELECT * FROM `cast` WHERE `seriesId` = #{id}
    </select>
</mapper>